{
  "lineWidth": 80,
  "indentWidth": 2,
  "useTabs": true,
  "newLineKind": "lf",
  "comment.forceLeadingSpace": true,
  "cargo.applyConventions": true,
  "includes": [
    "**/*.{toml}"
  ],
  "excludes": [
    "**/lib",
    "**/node_modules",
    "**/dist",
    "**/target",
    "**/vendor",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/toml-0.5.4.wasm"
  ]
}
